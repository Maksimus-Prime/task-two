mixin developer-software(headername, headertheme, list)
    include ../header/header.pug
    include ../software-item/software-item.pug
    section.developer-software
        .developer-software__header
            +header(headername, headertheme)
        .developer-software__left-column
            each item in list.software.left
                .developer-software__item
                    +software-item(item.name, item.value)
        .developer-software__right-column
            each item1 in list.software.right
                .developer-software__item
                    +software-item(item1.name, item1.value)