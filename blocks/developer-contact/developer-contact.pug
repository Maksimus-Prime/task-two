include ../header/header.pug
include ../contact-item/contact-item.pug
mixin developer-contact(params)
    -var items = params.contactItems ? params.contactItems : [];
    section.developer-contact
        .developer-contact__header
            +header(params.headerName, params.headerTheme)
        each item in items
            .developer-contact__item
                +contact-item(item.name, item.value, item.type, item.typevalue)